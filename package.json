    "Actions",
    "Go test"
  ],
  "bugs": {
    "url": "https://github.com/walkersumida/actions-go-test-coverage-checker/issues"
  },
  "homepage": "https://github.com/walkersumida/actions-go-test-coverage-checker#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@vercel/ncc": "^0.34.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
